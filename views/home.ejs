<%-include('partials/header.ejs')-%>

<h1>Home</h1>

<p><%=startingContent%></p>

<form class="" action="/" method="post">
  <%post.forEach((item, i) => {%>
    <h1 name="postTitle"><%=item.title%></h1>
    <p class="contentBody"><%=item.content.substring(0,100)+" ..."%><a href="/posts/<%=item.title%>">Read More</a></p>
  <%});%>
</form>


<%-include('partials/footer.ejs')-%>
